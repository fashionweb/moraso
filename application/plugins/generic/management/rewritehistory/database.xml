<?xml version="1.0" encoding="UTF-8"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="schema.xsd">
    <table name="rewrite_history" engine="InnoDB" since="2.2.1.1">
        <field name="id" type="int(10) unsigned" default="null" autoincrement="true"
            primary="true"/>
        <field name="idclient" type="int(10) unsigned" default="0"
            comment="Client, the entry belongs to.">
            <constraint table="clients" column="idclient" onupdate="no action" ondelete="cascade"/>
        </field>
        <field name="url" type="varchar(255)" default="null" 
            comment="Full URL of the article with the specified idartlang."/>
        <field name="target" type="varchar(255)" default="null" 
            comment="The article or category the entity belongs to."/>
        <field default="0" name="manualentry" type="tinyint(4) unsigned"
            comment="1 indicates a manual entry made with the plugin."/>
        <field name="created" type="datetime" default="null"/>
    </table>
</database>
